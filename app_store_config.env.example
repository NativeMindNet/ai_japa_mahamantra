# App Store Configuration Template
# Copy this file to app_store_config.env and fill in your values
# NEVER commit app_store_config.env to git!

# App Store Connect Credentials
APP_STORE_CONNECT_ISSUER_ID=your-issuer-id-here
APP_STORE_CONNECT_KEY_ID=your-key-id-here
APP_STORE_CONNECT_API_KEY_PATH=path/to/your/AuthKey_XXXXXXXXXX.p8

# Apple Developer Team
APPLE_TEAM_ID=your-team-id-here
APPLE_DEVELOPER_EMAIL=your-email@example.com

# App Information
APP_BUNDLE_ID=com.yourcompany.yourapp
APP_NAME=Your App Name
APP_SCHEME=yourapp

# Certificates & Provisioning
MATCH_GIT_URL=git@github.com:yourcompany/certificates.git
MATCH_PASSWORD=your-match-password-here
MATCH_TYPE=appstore

# Fastlane
FASTLANE_USER=your-fastlane-email@example.com
FASTLANE_PASSWORD=your-fastlane-password-here
FASTLANE_APPLE_APPLICATION_SPECIFIC_PASSWORD=your-app-specific-password-here

# Google Play (for cross-platform deployment)
GOOGLE_PLAY_JSON_KEY_PATH=path/to/your/google-play-api-key.json
ANDROID_KEYSTORE_PATH=path/to/your/upload-keystore.jks
ANDROID_KEYSTORE_PASSWORD=your-keystore-password-here
ANDROID_KEY_ALIAS=upload
ANDROID_KEY_PASSWORD=your-key-password-here

# Optional: Firebase
FIREBASE_APP_ID_IOS=your-firebase-app-id-ios
FIREBASE_APP_ID_ANDROID=your-firebase-app-id-android
FIREBASE_TOKEN=your-firebase-token-here

# Optional: Analytics
SENTRY_DSN=your-sentry-dsn-here
MIXPANEL_TOKEN=your-mixpanel-token-here

# Optional: Other Services
SLACK_WEBHOOK_URL=your-slack-webhook-url-here



